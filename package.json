{
  "name": "alimentary",
  "version": "1.0.0",
  "description": "client side weight loss monitoring app",
  "license": "MIT",
  "author": "nadia.m",
  "source": "public/index.html",
  "private": true,
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "test": "jest --env=jsdom",
    "test:cov": "jest --env=jsdom --coverage",
    "format": "prettier --write \"src/**/**/.{js, jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "client": "webpack serve --config webpack.config.dev.js --hot --open",
    "build": "webpack --config webpack.config.prod.js",
    "server": "nodemon backend/server.js",
    "clean": "rm -rf .cache dist node_modules yarn.lock yarn-error.log"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Naddiya/alimentary-client.git"
  },
  "keywords": [
    "open-source",
    "health",
    "reactJS",
    "javascript"
  ],
  "bugs": {
    "url": "https://github.com/Naddiya/alimentary-client/issues"
  },
  "homepage": "https://github.com/Naddiya/alimentary-client#readme",
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.18.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@testing-library/dom": "^8.13.0",
    "@testing-library/react": "^13.0.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^14.0.4",
    "@types/jest": "^27.4.1",
    "babel-jest": "^28.1.3",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.7.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-react": "^7.29.4",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^28.1.3",
    "mini-css-extract-plugin": "^2.6.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "react-test-renderer": "^18.2.0",
    "sass": "^1.54.0",
    "sass-loader": "^13.0.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.3"
  },
  "dependencies": {
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/material": "^5.9.3",
    "@reduxjs/toolkit": "^1.8.3",
    "express": "^4.18.1",
    "express-async-handler": "^1.2.0",
    "mongoose": "^6.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0"
  }
